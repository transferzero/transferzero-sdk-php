{
    "name": "transferzero/transferzero-php-sdk",
    "version": "1.0.0",
    "description": "TransferZero Client Library for PHP",
    "homepage": "http://api.transferzero.com/documentation",
    "license": "MIT",
    "authors": [
        {
            "name": "TransferZero",
            "homepage": "https://www.transferzero.com",
            "email": "engineers@transferzero.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "TransferZero\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "TransferZero\\" : "test/" }
    }
}
